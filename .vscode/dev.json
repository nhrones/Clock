{
   "simple": {
      "DEV": true,
      "Port": 80,
      "Serve": ""
   },
   "Build": {
      "DEV": true,
      "Entry": [
         "./src/main.ts"
      ],
      "Minify": false,
      "Out": "dist",
      "BundleName": "bundle"
   },
   "hot": {
      "BundleName": "bundle.js",
      "DEV": true,
      "Entry": [
         "./src/main.ts"
      ],
      "Minify": false,
      "OutPath": "dist",
      "Port": 80,
      "Serve": ".",
      "HtmlName": "index.html",
      "Watch": [
         "src",
         "dist"
      ]
   }
}